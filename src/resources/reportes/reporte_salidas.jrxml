	<parameter name="fecha_inicio" class="java.sql.Date"/>
	<parameter name="fecha_fin" class="java.sql.Date"/>
	<queryString language="SQL">
		<![CDATA[SELECT p.codigo AS codigo_producto,
       p.nombre AS producto,
       p.descripcion AS descripcion,
       p.precio AS precio,
       p.stock AS stock,
       p.unidad_medida AS unidad_medida,
       c.nombre AS categoria,
       pr.nombre AS proveedor,
       SUM(mi.cantidad) AS total_salidas
FROM MovimientoInventario mi
JOIN Producto p ON mi.codigo_producto = p.codigo
JOIN Categoria c ON p.id_categoria = c.id_categoria
JOIN Proveedor pr ON p.id_proveedor = pr.id_proveedor
WHERE mi.tipo = 'Salida'
  AND mi.fecha BETWEEN $P{fecha_inicio} AND $P{fecha_fin}
GROUP BY p.codigo, p.nombre, p.descripcion, p.precio, p.stock, p.unidad_medida, c.nombre, pr.nombre
ORDER BY total_salidas DESC]]>
	</queryString>
